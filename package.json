{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Portfolio listing all notable projects that I have worked on.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "fkill :9000 :3000 -s && concurrently \"npm:backend\" \"npm:frontend\"",
    "frontend": "webpack serve --open",
    "backend": "node index.js",
    "start": "npm run backend",
    "server": "nodemon index.js",
    "webpack": "process.env.NODE_ENV=production webpack",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "reset": "npm run rollback && npm run migrate && npm run seed",
    "seed": "knex seed:run",
    "initialmig": "npx knex migrate:make initial-migration",
    "cleanup": "knex seed:run --specific=01-cleanup.js",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "migup": "knex migrate:up",
    "migdown": "knex migrate:down",
    "stopserver": "npx kill-port 9000"
  },
  "repository": {
    "type": "git",
    "url": "git+http://github.com/dilemmaemma/portfolio.git"
  },
  "author": "dilemmaemma",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "fkill-cli": "^7.1.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "msw": "^1.3.2",
    "nodemon": "^3.0.1",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "antd": "^5.9.4",
    "axios": "^1.5.1",
    "cors": "^2.8.5",
    "db": "^4.0.1",
    "dotenv": "^16.3.1",
    "env": "^0.0.2",
    "express": "^4.18.2",
    "knex": "^3.0.1",
    "knex-cleaner": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "sqlite3": "^5.1.6",
    "styled-components": "^6.0.8",
    "webpack-cli": "^5.1.4",
    "yup": "^1.3.2"
  },
  "engines": {
    "node": ">=16.x",
    "npm": ">=8.x"
  }
}
