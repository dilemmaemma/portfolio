{
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "plugins": [
    "react-hooks"
  ],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "semi": "error",
    "no-unused-vars": "warn"
  },
  "ignorePatterns": ["src/__tests__/**", "**/*.spec.js"],
  "extends": "react-app"
}
